<h1>Update this blog </h1>
<%= form_with(model: @boot_app, local: true) do |form| %>

<div class="function_name">
    <%= form.label :Name %>
    <%= form.text_field :name %> 
</div>
<br>
<div class="function_name">
    <%= form.label :Email %>
    <%= form.text_field :email %> 
</div>
<br>
<div class="function_name">
    <%= form.label :Content %>
    <%= form.text_field :content %> 
</div>
<br>

<%= form.submit %>




<% end %>